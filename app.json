{
  "expo": {
    "name": "Steam Friends",
    "slug": "steamloginapp",
    "version": "1.0.0",
    "scheme": "steamloginapp",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a202c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.steamfriends.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "Steam Login",
            "CFBundleURLSchemes": [
              "steamloginapp"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a202c"
      },
      "package": "com.steamfriends.app",
      "permissions": [
        "INTERNET"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "steamloginapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}